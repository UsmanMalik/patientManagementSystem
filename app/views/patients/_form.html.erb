

                <div class="row">
                    <div class="col-sm-12">
                        <div class="panel panel-default">
                            <div class="panel-heading"><h3 class="panel-title"></h3></div>
                            <div class="panel-body">
<!--                                 <form class="form-horizontal" role="form"> -->
                                <p "form-horizontal" role="form">
                                <%= form_for(@patient) do |f| %>
                                    
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Name</label>
                                        <div class="col-md-10">
                                      <%= f.text_field :name , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Email</label>
                                        <div class="col-md-10">
                                      <%= f.text_field :email , class: "form-control" %>
                                    </div>
                                    </div><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Age</label>
                                        <div class="col-md-10">
                                      <%= f.number_field :age , class: "form-control" %>
                                    </div><br /><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Phone</label>
                                        <div class="col-md-10">
                                      <%= f.text_field :phone , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Date of Birth</label>
                                        <div class="col-md-10">                
                                      <%= f.date_select :date_of_birth , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Address</label>
                                        <div class="col-md-10">
                                          <%= f.text_area :address  , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br /><br /><br />                        
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Sex</label>
                                        <div class="col-md-10">         
                                      <%= f.text_field :sex , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br />
                                    <div class="form-group">
                                        <label class="col-md-2 control-label">Is Sms</label>
                                        <div class="col-md-10">
                                      <%= f.check_box :isSms , class: "form-control" %>
                                    </div>
                                    </div><br /><br /><br />
                                    <div class="form-group">
<!--                                         <label class="col-md-2 control-label">Submit</label> -->
                                        <div class="col-md-10">
                                        <button type="submit" class="btn btn-effect-ripple btn-primary">Submit</button>
                                     
                                    </div>
                                    </div>                                       
                               <% end %>
                               </p>

<!--                                 </form>
 -->                            </div> <!-- panel-body -->
                        </div> <!-- panel -->
                    </div> <!-- col -->
                </div> <!-- End row -->





